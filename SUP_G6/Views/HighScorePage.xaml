<Page x:Class="SUP_G6.Views.HighScorePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:SUP_G6.Views"
      mc:Ignorable="d" 
      d:DesignHeight="1080" d:DesignWidth="1920"
      Title="HighScorePage">

    <Grid>

        <Border
            Background="White"
            CornerRadius="70"
            Width="1000"
            Height="810"
            Opacity="0.76"
            >


        </Border>
        <StackPanel>
            <Label Content="Highscore" HorizontalAlignment="Center" Margin="0 100 0 40" FontSize="60"/>
            <ListBox ItemsSource="{Binding Players}" BorderBrush="#FF0D319B" Height="442" Width="630"/>
            <Button Content="Back" Height="62" Width="284" Margin="0 30 0 0" Click="Button_Click"/>


        </StackPanel>

    </Grid>
</Page>
